/* setting.wxss */
page{
  background-color: #f1f1f1;
}
.setting{
  width: 100%;
  background-color: #fff;
}
.setting .set-item{
  width: 90%;
  padding: 0 5%;
  margin-top: 20rpx;
}
.setting .set-item .set-list{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #f1f1f1;
}
.setting .set-item .set-list navigator{
  width: 100%;
  line-height: 100rpx;
  display: flex;
  font-size: 30rpx;
  color: #333;
}
.setting .set-item .set-list navigator view{
  font-size: 30rpx;
  color: #333;
}
.exit{
  width: 90%;
  padding: 0 5%;
  background-color: #ff671e !important;
  margin-top: 50rpx;
  color: #fff !important;
}
